matchInfo
    -for(var i=0;i<10;i++)
        -var game_times = 10;
        div(class="Win_Fail_COUNT" style="display:none;") #{i}
        .content_wrap.col-12
            .gameRecord_wrap.col-12.mw-1200.col-center
                .gameRecord_con.col-12
                    .gameRecord_inner.col-12
                        .gameRecord_info
                            .gameRecord_info_win_fail_check #{result[i+game_times].gameWinFail}
                            script.
                                var win_fail_number = #{i};
                                $(`.gameRecord_info_win_fail_check`).attr("class","win_fail_check"+ #{i});
                                $(`.gameRecord_wrap`).attr("class","gameRecord_wrap"+ #{i} +" col-12" + " mw-1200" + " col-center"+ " gameRecord_wrap_css");
                            .gameRecord_info_time #{result[i+game_times].gameTime}분
                        .gameRecord_champ
                            .gameRecord_champ_img
                                img(src=result[i+game_times].champ_img[i])
                            .gameRecord_champ_name #{result[i+game_times].champ_name[i]}
                            .gameRecord_spells
                                .gameRecord_spell01
                                    img(src=result[i+game_times].spell.spell1)
                                .gameRecord_spell02
                                    img(src=result[i+game_times].spell.spell2)
                        .gameRecord_KDA
                            .gameRecord_kda #{result[i+game_times].kills}/ #{result[i+game_times].deaths}/ #{result[i+game_times].assists}
                            .gameRecord_average #{result[i+game_times].kda}:1 평점
                        .gameRecord_levelCS
                            .gameRecord_level 레벨 #{result[i+game_times].level}
                            .gameRecord_CS #{result[i+game_times].total_cs} CS
                        .gameRecord_items
                            .gameRecord_items_top
                                div 
                                    img(src=result[i+game_times].item[0] onerror="this.src='/images/gray-background.jpg';this.style.opacity=0.5;")
                                div 
                                    img(src=result[i+game_times].item[1] onerror="this.src='/images/gray-background.jpg';this.style.opacity=0.5;")
                                div 
                                    img(src=result[i+game_times].item[2] onerror="this.src='/images/gray-background.jpg';this.style.opacity=0.5;")
                                div 
                                    img(src=result[i+game_times].item[3] onerror="this.src='/images/gray-background.jpg';this.style.opacity=0.5;")
                            .gameRecord_items_bottom
                                div
                                    img(src=result[i+game_times].item[4] onerror="this.src='/images/gray-background.jpg';this.style.opacity=0.5;")
                                div
                                    img(src=result[i+game_times].item[5] onerror="this.src='/images/gray-background.jpg';this.style.opacity=0.5;")
                                div
                                    img(src=result[i+game_times].item[6] onerror="this.src='/images/gray-background.jpg';this.style.opacity=0.5;")
                        .gameRecord_summoner_names            
                            .gameRecord_team01 
                                .gameRecord_row #{result[i][0]}
                                .gameRecord_row #{result[i][1]}
                                .gameRecord_row #{result[i][2]}
                                .gameRecord_row #{result[i][3]}
                                .gameRecord_row #{result[i][4]}
                            .gameRecord_team02
                                .gameRecord_row #{result[i][5]}
                                .gameRecord_row #{result[i][6]}
                                .gameRecord_row #{result[i][7]}
                                .gameRecord_row #{result[i][8]}
                                .gameRecord_row #{result[i][9]}